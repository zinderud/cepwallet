{
  "name": "cepwallet",
  "version": "0.1.0",
  "description": "Privacy-First Hardware Wallet with Ethereum & Kohaku Integration using Tauri",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "scripts": {
    "dev": "tauri dev",
    "build": "tauri build",
    "start": "tauri dev",
    "test": "echo \"No tests yet\"",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "clean": "rm -rf dist node_modules",
    "prepare": "husky install",
    "precommit": "lint-staged"
  },
  "dependencies": {},
  "devDependencies": {
    "concurrently": "^8.2.0",
    "husky": "^8.0.0",
    "lint-staged": "^15.2.0",
    "prettier": "^3.1.0",
    "typescript": "^5.3.0",
    "tauri": "^1.0.0"
  },
  "keywords": [
    "ethereum",
    "wallet",
    "trezor",
    "hardware-wallet",
    "privacy",
    "kohaku",
    "railgun",
    "zero-knowledge",
    "web3",
    "tauri"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/zinderud/cepwallet.git"
  },
  "homepage": "https://github.com/zinderud/cepwallet",
