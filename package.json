{
  "name": "cepwallet",
  "version": "0.1.0",
  "description": "Privacy-First Hardware Wallet with Ethereum & Kohaku Integration using Tauri",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "tauri": "tauri",
    "tauri:dev": "tauri dev",
    "tauri:build": "tauri build",
    "test": "echo \"No tests yet\"",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "clean": "rm -rf dist node_modules",
    "prepare": "husky install",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "@tauri-apps/api": "^2.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^2.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.2.0",
    "concurrently": "^8.2.0",
    "eslint": "^8.0.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.0",
    "husky": "^8.0.0",
    "lint-staged": "^15.2.0",
    "prettier": "^3.1.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0"
  },
  "keywords": [
    "ethereum",
    "wallet",
    "trezor",
    "hardware-wallet",
    "privacy",
    "kohaku",
    "railgun",
    "zero-knowledge",
    "web3",
    "tauri"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/zinderud/cepwallet.git"
  },
  "homepage": "https://github.com/zinderud/cepwallet",
  "bugs": "https://github.com/zinderud/cepwallet/issues",
  "license": "GPL-3.0",
  "author": "CepWallet Contributors"
}